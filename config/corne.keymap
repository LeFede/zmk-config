        keymap {
                compatible = "zmk,keymap";

                default_layer {
                        display-name = "Base";
// -----------------------------------------------------------------------------------------
// |NONE |  Q  |  W  |  E  |  R  |  T  |   |  Y  |  U   |  I  |  O  |  P  |NONE |
// |NONE | A/GUI| S/ALT| D/SHFT| F/CTRL|  G  |   |  H  | J/CTRL| K/SHFT| L/ALT| ;/GUI|NONE |
// |NONE |  Z  |  X  |  C  |  V  |  B  |   |  N  |  M   |  ,  |  .  |  /  |NONE |
//                  |     | NUM | SPC |   | ENT/NAV | EDIT |     |
                        bindings = <
   &none  &lt 7 Q      &lt 6 W     &kp E        &kp R         &lt 5 T    &kp Y &kp U         &kp I        &kp O       &kp P        &none
   &none  &hm LGUI A   &hm LALT S  &hm LSHFT D  &hm LCTRL F   &lt 4 G    &h_arrow &hm_j RCTRL 0   &hm_k RSHFT 0  &hm_l RALT 0  &hm RGUI SEMI &none
   &none  &kp Z        &kp X       &kp C        &kp V         &kp B      &kp N &kp M         &kp COMMA    &kp DOT     &kp FSLH     &none
                                                &mo 8 &mo 1 &kp SPACE   &lt 3 RET &lt 2 BSPC &kp DEL
                        >;
                };
                numpad_layer {
                        display-name = "Numpad";
// -----------------------------------------------------------------------------------------
// |NONE |     |     |  [  |  ]  |     |   |     |  7  |  8  |  9  |     |NONE |
// |NONE |  {  |  }  |  (  |  )  |     |   |     |  4  |  5  |  6  |     |NONE |
// |NONE |     |     |     |     |     |   |  0  |  1  |  2  |  3  |     |NONE |
//                  |     |     |     |   |     |     |     |
                        bindings = <
   &none  &trans       &trans       &kp LBKT     &kp RBKT     &trans         &trans &kp N7 &kp N8 &kp N9 &trans &none
   &none  &kp LBRC     &kp RBRC     &kp LPAR     &kp RPAR     &trans         &trans &kp N4 &kp N5 &kp N6 &trans &none
   &none  &trans       &trans       &trans       &trans       &trans         &kp N0 &kp N1 &kp N2 &kp N3 &trans &none
                                                 &trans     &trans       &trans      &trans  &trans   &trans
                        >;
                };

